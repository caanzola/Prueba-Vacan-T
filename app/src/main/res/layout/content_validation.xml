<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:showIn="@layout/activity_validation"
        tools:context=".Register.ValidationActivity">

    <TextView
        android:text="@string/id_type"
        android:layout_width="134dp"
        android:layout_height="45dp"
        android:gravity="center_vertical"
        android:id="@+id/id_type_register"
        android:layout_marginTop="52dp"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        android:layout_marginStart="32dp" app:layout_constraintStart_toStartOf="parent"
        android:layout_marginEnd="8dp" app:layout_constraintEnd_toStartOf="@+id/spinner_id_type"
        android:textSize="18sp" android:layout_marginBottom="97dp" />
    <Spinner
        android:layout_width="150dp"
        android:layout_height="45dp"
        android:id="@+id/spinner_id_type"
        android:layout_marginTop="52dp"
        app:layout_constraintTop_toBottomOf="@+id/textView6" app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="32dp" android:layout_marginStart="8dp"
        app:layout_constraintStart_toEndOf="@+id/id_type_register" android:layout_marginBottom="97dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>